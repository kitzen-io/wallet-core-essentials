{
  "name": "@kitzen/wallet-core-essentials",
  "version": "1.1.23",
  "description": "Kitzen Wallet Core Essentials is an open-source library, provides reusable components and core cryptographic wallet functionality for a different blockchains. It is a core part of Kitzen Wallet.",
  "author": "kitzen <kitzen.io>",
  "homepage": "https://github.com/kitzen-io/wallet-core-essentials#readme",
  "license": "MIT",
  "scripts": {
    "postinstall": "patch-package",
    "build": "rm -rf lib && tsc",
    "test": "jest",
    "watch": "rm -rf lib && tsc --watch"
  },
  "engines": {
    "node": ">=16"
  },
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kitzen-io/wallet-core-essentials.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": "./lib/index.js"
  },
  "files": [
    "lib",
    "patches"
  ],
  "dependencies": {
    "@kitzen/data-transfer-objects": "^1.0.57",
    "@noble/secp256k1": "1.6.3",
    "@tronscan/client": "^0.2.81",
    "bignumber.js": "^9.1.1",
    "bip32": "3.0.1",
    "bitcoin-address-validation": "^2.2.3",
    "bitcoinjs-lib": "^6.1.3",
    "bitcoinjs-message": "^2.2.0",
    "create-hmac": "^1.1.7",
    "ecpair": "2.0.1",
    "ethers": "6.7.0",
    "events": "3.3.0",
    "patch-package": "^7.0.2",
    "stream-browserify": "^3.0.0"
  },
  "peerDependencies": {
    "@kitzen/assets": "*"
  },
  "devDependencies": {
    "@kitzen/assets": "^1.0.18",
    "@types/create-hmac": "^1.1.0",
    "@types/google-protobuf": "^3.15.6",
    "@types/jest": "^26.0.0",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "bip39": "3.0.4",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^26.0.0",
    "tronweb": "^5.3.0",
    "ts-jest": "^26.0.0",
    "typescript": "^5.1.6"
  }
}
